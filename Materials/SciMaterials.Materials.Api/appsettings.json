{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FilesApiSettings": {
    "BasePath": ".\\tmp\\Files",
    "MaxFileSize": 107374182400,
    "Separator": ","
  },
  "ConnectionStrings": {
    "SqlServer.Debug": "Server=(localdb)\\MSSQLLocalDB;Database=SciMaterials.Materials"
  },
  "DatabaseSettings": {
    "Provider": "SqlServer.Debug",
    "RemoveAtStart": "true",
    "UseDataSeeder": "true"
  },
  "LinkOptions": {
    "HashStringLength": 5,
    "HashAlgorithm": "MD5",
    "Encoding": "UTF-32",
    "ConcurrentDbTimeout": 100,
    "ConcurrentDbTryCount": 10
  }
}
